{
    "manifest_version": 2,
    "icons": {
        "128": "img/icon_big.png"
    },
    "description": "本擴充功能用於pineapple",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html",
        "default_title": "開始一趟探縮房屋的旅程"
    },
    "content_scripts": [
        {
            "matches": [
                "*://rent.591.com.tw/rent-detail-*.html"
            ],
            "css": [
                "css/jsInjection.css"
            ],
            "js": [
                "js/comment.js", "js/bom.js", "js/crawler.js", "js/index.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "tabs",
        "https://rent.591.com.tw/*"
    ],
    "name": "租屋天眼通",
    "version": "1.0.0"
}